{
    "name": "@cloud9trader/websocket-api-client",
    "version": "0.0.6",
    "description": "Client adaptor for Cloud9Trader's realtime API for use in web browsers and NodeJS",
    "main": "dist/main.js",
    "module": "dist/main.js",
    "browser": "dist/main.js",
    "types": "dist/main.d.ts",
    "scripts": {
        "prepublish": "npm run build",
        "build": "tsc",
        "bundle": "rollup dist/main.js --file dist/bundle.js --format iife --compact",
        "build2": "rollup -c",
        "watch": "rollup -c -w"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Cloud9Trader/websocket-api-client.git"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/polyfill": "^7.4.4",
        "@types/ws": "^6.0.1",
        "isomorphic-fetch": "^2.2.1",
        "isomorphic-ws": "^4.0.1",
        "ws": "^7.1.1"
    },
    "devDependencies": {
        "rollup": "^1.13.1",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^10.0.0",
        "rollup-plugin-node-resolve": "^5.0.1",
        "rollup-plugin-typescript": "^1.0.1",
        "tslib": "^1.9.3",
        "typescript": "^3.5.1"
    },
    "files": [
        "dist"
    ]
}
